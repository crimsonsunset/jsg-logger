{
  "projectName": "JSG Logger",
  "globalLevel": "info",
  "autoRegister": true,
  "metaLogging": true,
  "format": {
    "style": "brackets",
    "componentCase": "upper",
    "timestamp": "HH:mm:ss.SSS"
  },
  "timestampMode": "absolute",
  "display": {
    "timestamp": true,
    "emoji": true,
    "component": true,
    "level": false,
    "message": true,
    "jsonPayload": true,
    "stackTrace": true
  },
  "redact": {
    "paths": ["password", "token", "*key", "*secret", "*apiKey", "*api_key"],
    "censor": "[REDACTED]"
  },
  "levels": {
