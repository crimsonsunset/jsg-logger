{
  "projectName": "JSG Logger",
  "globalLevel": "info",
  "autoRegister": true,
  "format": {
    "style": "brackets",
    "componentCase": "upper",
    "timestamp": "HH:mm:ss.SSS"
  },
  "timestampMode": "absolute",
  "display": {
    "timestamp": true,
    "emoji": true,
    "component": true,
    "level": false,
    "message": true,
    "jsonPayload": true,
    "stackTrace": true
  },
  "levels": {
    "10": { "name": "TRACE", "emoji": "🔍", "color": "#6C7B7F" },
    "20": { "name": "DEBUG", "emoji": "🐛", "color": "#74B9FF" },
    "30": { "name": "INFO", "emoji": "✨", "color": "#00B894" },
    "40": { "name": "WARN", "emoji": "⚠️", "color": "#FDCB6E" },
    "50": { "name": "ERROR", "emoji": "🚨", "color": "#E17055" },
    "60": { "name": "FATAL", "emoji": "💀", "color": "#D63031" }
  },
  "components": {
    "core": { 
      "emoji": "🎯", 
      "color": "#4A90E2", 
      "name": "JSG-CORE",
      "level": "info"
    }
  },
  "fileOverrides": {
  }
}